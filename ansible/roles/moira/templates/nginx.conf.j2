server {
    listen 0.0.0.0:80 default_server; 
    location = /favicon.ico {
        alias {{ moira_web_path }}/favicon.ico;
    }
    #location / {
    #    root {{ moira_web_path }};
    #    index index.html;
    #}
    location /api/ {
        proxy_pass http://127.0.0.1:8081;
    }
    location / {
        proxy_pass http://127.0.0.1:8080;
    }
}
